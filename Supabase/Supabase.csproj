<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>netstandard2.0</TargetFramework>
		<PackOnBuild>true</PackOnBuild>
		<PackageId>supabase-csharp</PackageId>
		<Authors>Joseph Schultz &lt;joseph@acupofjose.com&gt;</Authors>
		<Description>A C# implementation of the Supabase client</Description>
		<Copyright>MIT</Copyright>
		<NeutralLanguage>en-US</NeutralLanguage>
		<PackageLicenseUrl>https://github.com/supabase/supabase-csharp/blob/master/LICENSE</PackageLicenseUrl>
		<Owners>Joseph Schultz &lt;joseph@acupofjose.com&gt;</Owners>
		<PackageProjectUrl>https://github.com/supabase/supabase-csharp</PackageProjectUrl>
		<Summary>A C# implementation of the Supabase client</Summary>
		<Title>Supabase</Title>
		<PackageIconUrl>https://avatars.githubusercontent.com/u/54469796?s=200&amp;v=4</PackageIconUrl>
		<PackageTags>supabase</PackageTags>
		<ReleaseVersion>0.2.3</ReleaseVersion>
		<PackageVersion>0.2.3</PackageVersion>
		<PackageReleaseNotes>Ported to netstandard2.0 to support a wider ranger of applications.

Breaking Change:
- Postgrest uses `Postgrest.IntRange` instead of `System.Range`</PackageReleaseNotes>
	</PropertyGroup>
	<PropertyGroup Condition=" '$(Version)' == '' ">
		<VersionPrefix Condition=" '$(VersionPrefix)' == '' ">0.2.3</VersionPrefix>
		<Version Condition=" '$(Version)' == '' ">$(VersionPrefix)</Version>
	</PropertyGroup>
	<ItemGroup>
		<PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
		<PackageReference Include="postgrest-csharp" Version="2.0.2" />
		<PackageReference Include="realtime-csharp" Version="2.0.4" />
		<PackageReference Include="gotrue-csharp" Version="2.1.5" />
		<PackageReference Include="MimeMapping" Version="1.0.1.37" />
	</ItemGroup>
	<ItemGroup>
	  <Folder Include="Storage\" />
	</ItemGroup>
	<ItemGroup>
	  <None Remove="MimeMapping" />
	</ItemGroup>
</Project>